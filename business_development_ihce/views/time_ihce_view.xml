                                    <label string="" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="receiving_record_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="impi" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="impi_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                </group>
                            </page>
                            <page string="Código de Barras">
                                <group coslpan="4" col="8" >
                                    <field name="requirements_sheet" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="requirements_sheet_percent" coslpan="2" required="1"/>
