                                    <field name="advice_company_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                </group>
                            </page>
                            <page string="Patentes">
                                <group coslpan="4" col="8" >
                                    <field name="letter_company" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="letter_company_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
