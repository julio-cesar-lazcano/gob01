                            </page>
                            <page string="FDA">
                                <group coslpan="4" col="8" >
                                    <field name="contact_laboratory" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
