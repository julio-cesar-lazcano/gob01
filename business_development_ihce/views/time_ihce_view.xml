                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="send_samples" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="send_samples_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="emits_table" coslpan="2" required="1"/>
                                    <label string="días" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                    <field name="emits_table_percent" coslpan="2" required="1"/>
                                    <label string="%" coslpan="2"/>
                                    <label string="" coslpan="2"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
<!--
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
-->
                </form>
            </field>
        </record>
        
        <record id="time_development_tree" model="ir.ui.view">
            <field name="name">time.development.tree</field>
            <field name="model">time.development</field>
            <field eval="16" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Tiempos">
                    <field name="description" />
                </tree>
            </field>
        </record>
        
        <record id="time_development_action" model="ir.actions.act_window">
            <field name="name">Tiempos</field>
            <field name="res_model">time.development</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="time_development_tree"/>
        </record>
    
        <menuitem id="submenu_tiempos" name="Configuración" parent="menu_desarrollo" sequence="5" groups="business_development_ihce.admin_desarrollo"/>
        
        <menuitem id="tiempos" name="Tiempos" parent="submenu_tiempos" sequence="0" action="time_development_action"/>

    </data>
</openerp>
