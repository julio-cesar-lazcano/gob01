        <record id="indicador_emprendimiento_tree" model="ir.ui.view">
            <field name="name">indicador.emprendimiento.tree</field>
            <field name="model">indicador.emprendimiento</field>
            <field eval="16" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Indicadores IHCE Emprendimiento" colors="green: porcentaje &gt; 60; orange: porcentaje &gt; 30; red: porcentaje &lt;= 30;">
                    <field name='name'/>
                    <field name='meta_anual'/>
                    <field name='enero'/>
                    <field name='febrero'/>
                    <field name='marzo'/>
                    <field name='trim1'/>
                    <field name='abril'/>
                    <field name='mayo'/>
                    <field name='junio'/>
                    <field name='trim2'/>
                    <field name='julio'/>
                    <field name='agosto'/>
                    <field name='septiembre'/>
                    <field name='trim3'/>
                    <field name='octubre'/>
                    <field name='noviembre'/>
                    <field name='diciembre'/>
                    <field name='trim4'/>
                    <field name='total'/>
                    <field name='porcentaje'/>
                </tree>
            </field>
        </record>

        <record id="indicador_emprendimiento_action" model="ir.actions.act_window">
            <field name="name">Indicadores IHCE Emprendimiento</field>
            <field name="res_model">indicador.emprendimiento</field>
            <field name="view_type">tree</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="indicador_emprendimiento_tree"/>
        </record>
    
